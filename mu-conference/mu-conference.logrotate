/var/log/jabberd2/mu-conference.log {
    daily
    rotate 7
    delaycompress
    compress
    notifempty
    missingok
    create 644 %JABBERDUSER% %JABBERDGROUP%
    sharedscripts
    postrotate
        /bin/kill -HUP `cat /var/run/jabberd2/mu-conference.pid 2>/dev/null` 2>/dev/null || true
    endscript
}
